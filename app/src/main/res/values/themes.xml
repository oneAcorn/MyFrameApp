<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyFrameApp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="MyQMUITheme" parent="QMUI.Compat.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimary</item>
    </style>

    <style name="APDefacto.Dark">
        <item name="android:background">@color/ap_black</item>
        <item name="backgroundColor">@color/ap_black</item>
        <item name="graphBackgroundColor">@color/ap_black</item>
        <item name="gridBackgroundColor">@color/ap_charcoal</item>
        <item name="domainLineColor">@color/off_white</item>
        <item name="rangeLineColor">@color/off_white</item>
        <item name="domainOriginLineColor">@color/off_white</item>
        <item name="rangeOriginLineColor">@color/off_white</item>
        <item name="lineLabelTextColorTop">@color/off_white</item>
        <item name="lineLabelTextColorBottom">@color/off_white</item>
        <item name="lineLabelTextColorLeft">@color/off_white</item>
        <item name="lineLabelTextColorRight">@color/off_white</item>
        <item name="titleTextColor">@color/ap_white</item>
        <item name="domainTitleTextColor">@color/ap_white</item>
        <item name="rangeTitleTextColor">@color/ap_white</item>
        <item name="legendTextColor">@color/ap_white</item>
    </style>
</resources>